{
  "name": "elobato-packstack",
  "version": "0.0.1",
  "author": "Daniel Lobato",
  "license": "Licensed under (Apache 2.0)",
  "summary": "This module only installs packstack, and the packstack repo from RDO, no parameters, no frills.",
  "source": "https://github.com/elobato/puppet-packstack",
